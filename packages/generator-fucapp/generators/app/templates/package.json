{
  "name": "<%= appname %>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "fuc-cli-service serve",
    "build": "fuc-cli-service build",
    "lint": "fuc-cli-service lint"
  },
  "dependencies": {
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "fuc-cli-service": "file:../fuc-cli/packages/cli-service"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
